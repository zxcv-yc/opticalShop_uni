	padding: 30rpx;
}
.cart_item {
	padding: 30rpx 20rpx;
	margin-bottom: 30rpx;
}
.cart_item_info {
	width: calc(100% - 218rpx);
}
.cart_item_title {
	width: calc(100% - 42rpx);
}
.text-FC3B00 {
	color: #FC3B00;
}
.bottom_bar {
	padding: 20rpx;
	width: 100%;
	position: fixed;
	bottom:50px;
}
