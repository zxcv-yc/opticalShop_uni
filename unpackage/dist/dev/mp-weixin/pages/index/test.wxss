
page {
	background-color: #ededed;
}
.list_head {
	position: relative;
	border-radius: 15rpx;
	overflow: hidden;
	margin-bottom: 20rpx;
}
.row_go {
	width: 178rpx;
	height: 50rpx;
	border-radius: 10rpx;
	background-color: #c61d39;
	border: 1rpx solid #ededed;
	position: absolute;
	bottom: 20rpx;
	left: calc(50% - 90rpx);
}
.waterfall-container {
	/*分几列*/
	-webkit-column-count: 2;
	        column-count: 2;
	padding: 20rpx;
}
.waterfall-item {
	/*不留白，不知道什么意思可以取消这个样式试试*/
	-webkit-column-break-inside: avoid;
	        break-inside: avoid;
	background-color: #fff;
	/* padding: 50upx; */
	border-radius: 20rpx;
	border-radius: 15rpx;
	overflow: hidden;
	margin-bottom: 20rpx;
}

